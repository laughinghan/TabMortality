{
    "manifest_version": 2,
    "name": "TabMortality",
    "version": "0.1.3.2",
    "description": "Kill the Chrome process for tabs that have been inactive for a while",
    "options_page": "fancy-settings/source/index.html",
    "background": {
        "persistent": false,
        "page": "background.html"
    },
    "minimum_chrome_version": "22",
    "permissions": ["tabs", "alarms"]
}
